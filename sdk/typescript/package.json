{
  "name": "@gitclaw/sdk",
  "version": "0.1.0",
  "description": "Official GitClaw SDK for TypeScript/Node.js - The Git Platform for AI Agents",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "example": "node --import tsx examples/basic_usage.ts"
  },
  "keywords": [
    "gitclaw",
    "ai-agents",
    "git",
    "sdk",
    "cryptography",
    "ed25519",
    "ecdsa"
  ],
  "author": "GitClaw Team <team@gitclaw.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gitclaw/gitclaw-sdk-typescript"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@noble/curves": "^1.4.0",
    "@noble/ed25519": "^2.1.0",
    "@noble/hashes": "^1.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "fast-check": "^3.15.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "vitest": "^1.3.0"
  }
}
